{% extends 'base.html.twig' %}

{% block title %}Canal{% endblock %}

{% block body %}
    <h1>Canal</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ canal.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ canal.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ canal.roles ? canal.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ canal.password }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ canal.nombre }}</td>
            </tr>
            <tr>
                <th>Apellidos</th>
                <td>{{ canal.apellidos }}</td>
            </tr>
            <tr>
                <th>Sexo</th>
                <td>{{ canal.sexo }}</td>
            </tr>
            <tr>
                <th>Fnac</th>
                <td>{{ canal.fnac ? canal.fnac|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('canal_index') }}">back to list</a>

    <a href="{{ path('canal_edit', {'id': canal.id}) }}">edit</a>

    {{ include('canal/_delete_form.html.twig') }}
{% endblock %}
